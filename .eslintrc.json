{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "no-console": "off"
    },
    "overrides": [
        {
          "files": [
            "**/*.test.js",
            "**/*.test.jsx"
          ]
        }
    ]
}
